{
    "compilerOptions": {
        "strict": true,
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "ESNext",
        "target": "es5",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "paths": {
            "*": [
                "./src/*"
            ]
        },
        "strictNullChecks": true
    },
    "ts-node": {
        "compilerOptions": {
            "module": "CommonJS"
        }
    },
    // "include": [
    //     "./config/jest/setupTests.ts"
    // ],
}